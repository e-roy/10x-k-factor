{
  "name": "10x-k-factor",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "drizzle:gen": "pnpm --filter @10x-k-factor/web drizzle:gen",
    "drizzle:push": "pnpm --filter @10x-k-factor/web drizzle:push",
    "drizzle:studio": "pnpm --filter @10x-k-factor/web drizzle:studio",
    "seed": "pnpm --filter @10x-k-factor/web seed"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.5.2",
    "prettier": "^3.0.0",
    "turbo": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.20.0"
}
